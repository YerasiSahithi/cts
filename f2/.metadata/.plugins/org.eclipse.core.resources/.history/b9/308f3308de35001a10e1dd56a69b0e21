package com;
import java.io.FileOutputStream;

public class FileOutputStreamExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
try {
	FileOutputStream fout=new FileOutputStream("C://");
    fout.write(65);    
    fout.close();    

}catch(Exception e) {
	System.out.println(e);
}
	}

}
